    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "gsap": "^3.12.3",
    "lil-gui": "^0.19.1",
