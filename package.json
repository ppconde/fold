{
  "name": "fold",
  "homepage": "https://ppconde.github.io/fold/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build --base=./",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "update-types": "npx supabase gen types typescript --project-id \"dvwjchfzwfhxuqlixomh\" > src/types/database.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.46.1",
    "gsap": "^3.12.3",
    "lil-gui": "^0.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stats.js": "^0.17.0",
    "three": "^0.170.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.169.0",
    "@types/webgl2": "^0.0.11",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "3.4.1",
    "sass": "^1.82.0",
    "supabase": "^1.219.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-plain-text": "^1.4.2",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.8"
  }
}
