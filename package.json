{
  "name": "fold",
  "homepage": "https://ppconde.github.io/fold/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build --base=./",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "update-types": "npx supabase gen types typescript --project-id \"dvwjchfzwfhxuqlixomh\" > src/types/database.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "gsap": "^3.12.3",
    "lil-gui": "^0.19.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stats.js": "^0.17.0",
    "three": "^0.168.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.168.0",
    "@types/webgl2": "^0.0.11",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "sass": "^1.78.0",
    "supabase": "^1.192.5",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-plain-text": "^1.4.2",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.0.5"
  }
}
