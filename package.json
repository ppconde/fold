    "react-dom": "^18.2.0",
    "stats.js": "^0.17.0",
    "three": "^0.159.0",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.22",
    "@types/three": "^0.159.0",
    "@types/webgl2": "^0.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sass": "^1.69.5",
    "supabase": "^1.123.4",
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vite-plugin-svgr": "^4.2.0"
